<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{1}:%L - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="net.schmizz.sshj" level="ERROR"/>

  <root level="INFO">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>